<launch>
  <node pkg="chibi_challenge" name="localpath" type="dwa" output="screen">
    <param name="max_speed" value="1.0"/>
    <param name="max_yawrate" value="1.0"/>
    <param name="max_accel" value="1.5"/>
    <param name="max_dyawrate" value="4.5"/>
    <param name="v_reso" value="0.01"/>
    <param name="yawrate_reso" value="0.05"/>
    <param name="dt" value="0.1"/>
    <param name="predict_time" value="3.0"/>
    <param name="robot_radius" value="0.25"/>
    <param name="alpha" value="6.5"/>
    <param name="beta" value="1.0"/>
    <param name="gamma" value="1.0"/>
    <param name="local_goal_point" value="1.5"/>
  </node>
</launch>
